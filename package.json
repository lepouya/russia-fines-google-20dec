  },
  "homepage": "https://github.com/lepouya/russia-fines-google-20dec",
  "type": "module",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "npm run debug && vite preview",
    "debug": "cross-env NODE_ENV=development tsc && vite build --mode development",
    "release": "cross-env NODE_ENV=production tsc && vite optimize --mode production && vite build --mode production",
    "github-release": "npm run release && vite build --mode production --emptyOutDir --outDir ../lepouya.github.io/russia-fines-google-20dec/",
    "test": "vitest",
    "update": "rm -rf node_modules && npm update"
  },
  "dependencies": {
    "@capacitor/app": "latest",
    "@capacitor/core": "latest",

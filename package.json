    "@ionic/react": "latest",
    "@ionic/react-router": "latest",
    "@ionic/storage": "latest",
    "break_eternity.js": "latest",
    "ionicons": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-router": "latest",
    "react-router-dom": "latest"
  },
  "devDependencies": {
    "@capacitor/cli": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-router": "latest",
    "@types/react-router-dom": "latest",
    "@vitejs/plugin-legacy": "latest",
